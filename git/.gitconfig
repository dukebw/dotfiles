[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Brendan Duke
	email = brendanw.duke@gmail.com
[core]
	editor = nvim
	pager = delta
[blame]
	pager = delta
[interactive]
	diffFilter = delta --color-only
[delta]
	navigate = true
	light = false
	side-by-side = true
	line-numbers = false
	hyperlinks = true
[diff]
	colorMoved = default
[merge]
	conflictstyle = diff3
[pull]
	rebase = false
